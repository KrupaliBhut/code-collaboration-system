  <body>
    <%- include("threetabs") -%>
    <div class="input-group">
        <input type="search" class="form-control rounded" placeholder="Search" aria-label="Search" aria-describedby="search-addon"   onChange='return change(this)'/>
        <button type="button" class="btn btn-outline-primary">search</button>
      </div>
      <div id="new"></div>
  </body>
  <script>
    </script>
<script>
   
    async function change(name) {
var div_ele = document.getElementById('new');
var user = name.value;
console.log("name.value",name.value);
console.log("name<<<<: " + name);
const res =await fetch(`/collab?name=${user}`)
const results = await res.json();
const AllRepos = results.repos;
var html= ``;
AllRepos.forEach((element, i) => {
        let htmlSegmnt = ` <ul>
      ${element.username}
      
    </ul>
        `;
        html += htmlSegmnt;
      });
      div_ele.innerHTML = html;
      console.log("name.innerhtml<<<<<<",name.innerHTML);
}
</script>
  </script>
